@model IEnumerable<kisiselWebsitem.Models.Model.Kategori>
@{
    ViewBag.Title = "Admin Ana Sayfa";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="row">
    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
        <div class="info-box card blue-bg">
            <div class="card-body">

                <i class="fa fa-cloud-download"></i>
                <div class="count">@ViewBag.BlogSay</div>
                <div class="title">Blog Yazısı</div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
        <div class="info-box card brown-bg">
            <div class="card-body">
                <i class="fa fa-shopping-cart"></i>
                <div class="count">@ViewBag.KategoriSay</div>
                <div class="title">Kategori</div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
        <div class="info-box card green-bg">
            <div class="card-body">
                <i class="fa fa-cubes"></i>
                <div class="count">@ViewBag.YorumSay</div>
                <div class="title">Yorumlar</div>
            </div>
        </div>
    </div>
</div>


<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="text-center">
        <div class="credits">
            Designed by <a href="#" style="color:#000000;">Kişisel Websitem Admin</a>
        </div>
    </div>
</div>